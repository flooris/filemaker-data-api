{
    "name": "flooris/filemaker-data-api",
    "description": "Package to easily access the FileMaker Data API",
    "type": "library",
    "homepage": "https://github.com/flooris/filemaker-data-api",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Flooris",
            "email": "info@flooris.nl"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.5",
        "nesbot/carbon": "*"
    },
    "replace": {
        "illuminate/support": "*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Flooris\\FileMakerDataApi\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flooris\\FileMakerDataApi\\FileMakerDataApiServiceProvider"
            ]
        }
    }
}
